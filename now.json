{
  "name": "locally-server",
  "version": 2,
  "builds": [
    { "src": "public/index.html", "use": "@now/static" },
    { "src": "lib/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/index.html", "status": 301, "headers": { "Location": "/" } },
    { "src": "/(.*)", "dest": "/lib/index.js" }
  ],
  "regions": ["icn1", "hnd1"]
}
